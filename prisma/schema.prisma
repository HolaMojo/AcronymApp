generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SimpleAF {
  abbreviation_id      Int       @id @default(autoincrement())
  abbreviation         String
  meaning              Text
  keywords             Text
  context              Text
  email_for_questions  Text
}
